{
  "name": "with-ant-design",
  "version": "1.0.0",
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "export": "yarn clean && next build && next export",
    "deploy:github": "NODE_ENV=production DEPLOY_ENV=github yarn export && touch out/.nojekyll && gh-pages -t -d out",
    "clean": "rm -rf node_modules/.cache"
  },
  "dependencies": {
    "antd": "^3.16.1",
    "axios": "^0.18.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "less": "3.9.0",
    "less-vars-to-js": "1.3.0",
    "next": "^7.0.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-decorators": "^7.1.0",
    "@types/next": "^7.0.5",
    "@types/react": "^16.7.17",
    "@types/styled-components": "^4.1.12",
    "@types/styled-jsx": "^2.2.7",
    "@visuallylab/tslint-config-frontend": "^1.0.11",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-less": "^1.0.2-canary.2",
    "@zeit/next-typescript": "^1.1.1",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.0",
    "compression": "^1.7.3",
    "dotenv-webpack": "^1.5.7",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "tslint": "^5.15.0",
    "typescript": "^3.4.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": false,
    "trailingComma": "all"
  },
  "license": "ISC"
}
